{
    "name": "craigpaul/moneris-api",
    "description": "An easier way to consume the (truly awful) Moneris eSELECTplus API.",
    "keywords": ["moneris", "eselectplus", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Paul",
            "email": "craig@townfolio.co"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "phpunit/php-code-coverage": "^4.0",
        "symfony/var-dumper": "^3.1",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "CraigPaul\\Moneris\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/mocks/",
            "tests/TestCase.php"
        ],
        "files": [
            "tests/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
